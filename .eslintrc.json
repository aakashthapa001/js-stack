{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "rules": {
    "linebreak-style": [2, "windows"],
    "semi": [2, "never"],
    "no-unexpected-multiline": 2,
    "eol-last": [2, "never"],
    "compat/compat": 2
  },
  "env": {
    "jest": true
  }
}